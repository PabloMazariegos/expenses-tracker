{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
	"rules": {
		"space-before-function-paren": "off",
		"no-unused-vars": "warn",
		"@typescript-eslint/no-explicit-any": "warn",
		"no-multiple-empty-lines": "off",
		"no-tabs": "off",
		"eol-last": "off",
		"indent": [
			"warn",
			"tab"
		],
		"quotes": [
			"warn",
			"single"
		],
		"jsx-quotes": [
			"warn",
			"prefer-single"
		],
		"@typescript-eslint/no-unused-vars": [
			"warn",
			{
				"argsIgnorePattern": "^_",
				"varsIgnorePattern": "^_",
				"caughtErrorsIgnorePattern": "^_"
			}
		]
	}
}
